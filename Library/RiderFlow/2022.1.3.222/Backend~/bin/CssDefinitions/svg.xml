      <name value="alphabetic"/>
      <name value="hanging"/>
      <name value="mathematical"/>
      <name value="central"/>
      <name value="middle"/>
      <name value="text-after-edge"/>
      <name value="text-before-edge"/>
    </group>
  </property>

