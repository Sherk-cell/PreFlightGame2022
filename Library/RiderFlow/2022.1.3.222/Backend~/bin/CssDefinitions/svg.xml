<!-- Already declared in css3.0.xml

  <property id="opacity" declared-in="2.0">
    <group type="or">
      <number/>
      <percentage/>
    </group>
  </property>
-->

  <property id="overflow" declared-in="2.0">
    <group type="or">
      <name value="visible"/>
      <name value="hidden"/>
      <name value="scroll"/>
      <name value="auto"/>
    </group>
  </property>

