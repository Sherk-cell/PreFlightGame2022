  <!-- <property id="color"> already properly defined elsewhere -->

  <property id="color-interpolation" declared-in="2.0">
    <group type="or">
      <name value="auto"/>
      <name value="sRGB"/>
      <name value="linearRGB"/>
    </group>
  </property>

